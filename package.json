    "test:assets:integrated": "playwright test tests/e2e/asset-management-integrated.spec.ts",
    "test:campaigns": "playwright test tests/e2e/campaign-matrix-integrated.spec.ts",
    "test:rendering": "playwright test tests/e2e/rendering-pipeline-integrated.spec.ts",
    "test:portal": "playwright test tests/e2e/client-portal-integrated.spec.ts",
