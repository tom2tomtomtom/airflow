{
  "name": "airwave",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "NODE_OPTIONS=\"--max-old-space-size=8192\" next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "NODE_OPTIONS=\"--max-old-space-size=8192\" tsc --noEmit",
    "type-check:phase1": "tsc --noEmit -p tsconfig.migration-phase1.json",
    "type-check:phase2": "tsc --noEmit -p tsconfig.migration-phase2.json",
    "type-check:final": "tsc --noEmit -p tsconfig.migration-final.json",
    "migrate:types": "node scripts/migrate-types.js",
    "test:types": "node scripts/test-types.js",
    "fix:types": "node scripts/migrate-types.js && npm run type-check:phase1",
    "postinstall": "echo 'Install complete'",
    "prepare": "node -e \"if (process.env.CI !== 'true') { try { require('husky').add('.husky/pre-commit', 'npm run pre-commit'); } catch {} }\"",
    "workers": "tsx workers/index.ts",
    "worker:render": "tsx workers/index.ts --only=render",
    "worker:email": "tsx workers/index.ts --only=email",
    "worker:webhook": "tsx workers/index.ts --only=webhook",
