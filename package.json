    "test:e2e:headed": "playwright test --headed",
    "test:auth": "playwright test tests/e2e/auth-comprehensive-test.spec.ts",
    "debug:auth": "node scripts/debug-auth.js",
    "fix:typescript:all": "tsx scripts/fix-all-typescript-errors.ts",
    "count:errors": "tsx scripts/count-errors.ts",
    "test:comprehensive": "playwright test --config=playwright.config.comprehensive.ts",
    "test:comprehensive:headed": "playwright test --config=playwright.config.comprehensive.ts --headed",
    "test:comprehensive:ui": "playwright test --config=playwright.config.comprehensive.ts --ui",
    "test:comprehensive:enhanced": "node scripts/run-comprehensive-tests.js",
    "test:comprehensive:all": "node scripts/run-comprehensive-tests.js --suite all",
    "test:comprehensive:functional": "node scripts/run-comprehensive-tests.js --suite functional",
    "test:comprehensive:performance": "node scripts/run-comprehensive-tests.js --suite performance",
