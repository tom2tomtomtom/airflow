    "test:airwave:ui": "playwright test tests/e2e/airwave-comprehensive.spec.ts --ui",
    "test:airwave:debug": "playwright test tests/e2e/airwave-comprehensive.spec.ts --debug",
    "test:complete-workflow": "playwright test tests/e2e/complete-user-workflow.spec.ts --config=playwright.config.comprehensive-enhanced.ts",
    "test:performance-detailed": "playwright test tests/e2e/performance-accessibility.spec.ts --config=playwright.config.comprehensive-enhanced.ts --project=performance",
    "test:accessibility-detailed": "playwright test tests/e2e/performance-accessibility.spec.ts --config=playwright.config.comprehensive-enhanced.ts --project=accessibility",
    "test:mobile": "playwright test --config=playwright.config.comprehensive-enhanced.ts --project=mobile-chrome",
    "test:mobile:all": "playwright test --config=playwright.config.comprehensive-enhanced.ts --project=mobile-chrome --project=mobile-safari --project=tablet",
    "test:cross-browser": "playwright test tests/e2e/complete-user-workflow.spec.ts --config=playwright.config.comprehensive-enhanced.ts --project=chrome-functional --project=firefox-functional --project=safari-functional",
    "test:performance": "playwright test --config=playwright.config.comprehensive-enhanced.ts --project=performance",
    "test:visual": "playwright test --config=playwright.config.comprehensive.ts --project=visual",
    "test:accessibility": "playwright test --config=playwright.config.comprehensive-enhanced.ts --project=accessibility",
    "test:debug": "playwright test --config=playwright.config.comprehensive-enhanced.ts --headed --debug",
    "test:report": "playwright show-report test-results/html-report",
    "test:setup": "playwright test --config=playwright.config.comprehensive.ts --project=setup",
    "test:working": "playwright test --config=playwright.config.working.ts",
    "test:user": "playwright test tests/simple-user-test.spec.ts --config=playwright.config.working.ts",
    "test:discover-errors": "playwright test tests/comprehensive-error-discovery.spec.ts --config=playwright.config.working.ts --headed",
    "test:user-workflows": "playwright test tests/user-workflow-testing.spec.ts --config=playwright.config.working.ts --headed",
    "validate:env": "node scripts/validate-env.js",
    "validate:env:production": "node scripts/validate-env.js .env.production",
    "validate:production": "npm run validate:env:production && npm run build",
    "cleanup": "node scripts/cleanup.js",
    "cleanup:repo": "rm -f fix_*.js temp.txt test.php tsconfig.tsbuildinfo && rm -f src/pages/strategic-content-fixed.tsx src/pages/matrix.tsx.new",
    "monitor:setup": "node scripts/monitoring-setup.js",
    "health:check": "curl -f http://localhost:3000/api/health || exit 1",
    "perf:test": "node scripts/performance-testing.js",
    "smoke:test": "node scripts/smoke-tests.js",
    "smoke:test:prod": "node scripts/smoke-tests.js --url $PRODUCTION_URL",
    "production:checklist": "echo 'ðŸš€ Running production readiness checklist...' && npm run validate:production && npm run perf:test && npm run smoke:test",
    "strict:enable": "node scripts/enable-strict-mode.js",
    "fix:deprecations": "node scripts/fix-deprecations.js",
    "ci:check": "npm run lint && npm run type-check && npm test && npm run build",
    "ci:quick": "npm run lint && npm run type-check",
    "pre-commit": "npm run ci:quick",
    "pre-push": "npm run ci:check",
    "fix:lint": "node scripts/fix-lint-issues.js",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "audit:security": "node scripts/security-audit.js"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
